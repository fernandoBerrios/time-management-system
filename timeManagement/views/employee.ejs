<%- include header.ejs %>
<div class="container">
  <%- include msg.ejs %>
</div>
<div class="main-content-container container-fluid px-4">
  <!-- Page Header -->
  <div class="page-header row no-gutters py-4">
    <div class="col-12 col-sm-4 text-center text-sm-left mb-0">
      <span class="text-uppercase page-subtitle">Overview</span>
      <h3 class="page-title">All Users</h3>
    </div>
    <div class="col-12 col-sm-8 text-right mb-0">
      <a href='/user/<%= user.employeeID %>'><button type="button" class="mb-2 btn btn-success mr-2">Add
          User</button></a>
    </div>
  </div>
  <!-- End Page Header -->
  <!-- Default Table -->
  <div class="row">
    <div class="col">
      <div class="card card-small mb-4">
        <div class="card-header border-bottom">
          <h6 class="m-0">Total Employee Users: <%= allUser.length %></h6>
        </div>
        <div class="card-body p-0 pb-3 text-center">



          <table class="table mb-0">
            <thead class="bg-light">
              <tr>

                <th scope="col" class="border-0">Team Lead</th>
                <th scope="col" class="border-0">First Name</th>
                <th scope="col" class="border-0">Last Name</th>
                <th scope="col" class="border-0">Employee ID</th>
                <th scope="col" class="border-0">Department</th>
                <th scope="col" class="border-0">Job Title</th>
                <th scope="col" class="border-0">Email</th>
                <th scope="col" class="border-0">Verfrication Status</th>
                <th scope="col" class="border-0">Action</th>
              </tr>
            </thead>
            <tbody>
              <% for (var i = 0; i < allUser.length; i++) { %>
              <tr>
                <% if (allUser[i].level == 2) { %>
                <td><i class="material-icons">check_circle</i></td>
                <%} else { %>
                <td></td>
                <%} %>
                <td><%= allUser[i].firstName %></td>
                <td><%= allUser[i].lastName %></td>
                <td><%= allUser[i].employeeID %></td>
                <td><%= allUser[i].department %></td>
                <td><%= allUser[i].jobTitle %></td>
                <td><%= allUser[i].username %></td>
                <% if (allUser[i].verifyUserToken != undefined) { %>
                <td><i class="material-icons">close</i></td>
                <%} else { %>
                <td><i class="material-icons">check</i></td>
                <%} %>

                <td><button type="button" class="mb-2 btn btn-sm btn-success mr-1">Edit</button>
                  <button type="button" class="mb-2 btn btn-sm btn-danger mr-1">Del</button></td>
              </tr>
              <% } %>
            </tbody>
          </table>

        </div>
      </div>
    </div>
  </div>
  <!-- End Default Table -->
  <div class="row">
    <div class="col">
      <div class="card card-small mb-4">
        <div class="card-header border-bottom">
          <h6 class="m-0">Contractor Users</h6>
        </div>
        <div class="card-body p-0 pb-3 text-center">
          <table class="table mb-0">
            <thead class="bg-light">
              <tr>
                <th scope="col" class="border-0">#</th>
                <th scope="col" class="border-0">First Name</th>
                <th scope="col" class="border-0">Last Name</th>
                <th scope="col" class="border-0">Project</th>
                <th scope="col" class="border-0">End</th>
                <th scope="col" class="border-0">Phone</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>1</td>
                <td>Ali</td>
                <td>Kerry</td>
                <td>Web</td>
                <td>May 05, 2019</td>
                <td>303-107-0339</td>
              </tr>
              <tr>
                <td>2</td>
                <td>Clark</td>
                <td>Angela</td>
                <td>APP</td>
                <td>April 30, 2019</td>
                <td>660-850-1647</td>
              </tr>
              <tr>
                <td>3</td>
                <td>Jerry</td>
                <td>Nathan</td>
                <td>AI</td>
                <td>March 03, 2019</td>
                <td>909-214-4225</td>
              </tr>
              <tr>
                <td>4</td>
                <td>Colt</td>
                <td>Angela</td>
                <td>Blockchain</td>
                <td>July 24, 2019</td>
                <td>818-473-7416</td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>
</main>
</div>
</div>

